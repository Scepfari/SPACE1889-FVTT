l,"sort":0,"permission":{"default":0,"LyZmxZAxS35OQM6i":3},"flags":{}},{"_id":"X2tvEVsADY9FTnLi","name":"Begabung","type":"talent","img":"icons/svg/item-bag.svg","data":{"description":"","id":"begabung","publication":{"page":"171"},"page":"","level":{"value":1,"min":1,"max":1},"newBase":"","changedSkill":"","preconditionType":"nothing","preconditionName":"","isGroup":false,"preconditionLevel":0,"secondPreconditionType":"nothing","secondPreconditionName":"","secondPreconditionLevel":0,"isOrOperator":false,"bonus":"2","bonusTarget":"ueberleben","bonusTargetType":"skill","info":"+2 auf Fertigkeit","source":{"publication":"grw"}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"LyZmxZAxS35OQM6i":3},"flags":{"core":{"sourceId":"Compendium.space1889.talente.IE0cUp7Mofm3Q3wj"}}},{"_id":"5r24JFBjtlNn6WL8","name":"Mandibeln","type":"weapon","img":"icons/svg/sword.svg","data":{"description":"","weight":0,"id":"mandibeln","location":"koerper","price":"","skillId":"waffenlos","specializationId":"none","damage":2,"damageType":"lethal","strengthThreshold":"0","range":"","capacity":1,"capacityType":"default","rateOfFire":"","speed":"","isAreaDamage":false,"ammunitionType":""},"effects":[],"folder":"DTItef9d4RgeI9eK","sort":1500000,"permission":{"default":0,"LyZmxZAxS35OQM6i":3},"flags":{"core":{"sourceId":"Item.XKk5JkDdEWTohLJQ"}}},{"_id":"AtNDHkJXyDFvo0e8","name":"Umschlingen","type":"weapon","img":"icons/svg/sword.svg","data":{"description":"","weight":0,"id":"umschlingen","location":"koerper","price":"","skillId":"waffenlos","specializationId":"none","damage":2,"damageType":"nonLethal","strengthThreshold":"0","range":"","capacity":1,"capacityType":"default","rateOfFire":"","speed":"","isAreaDamage":false,"ammunitionType":""},"effects":[],"folder":"DTItef9d4RgeI9eK","sort":2800000,"permission":{"default":0,"LyZmxZAxS35OQM6i":3},"flags":{"core":{"sourceId":"Item.D9pHcp8iXlVNBQfh"}}},{"_id":"ZomP1SRLAb2GflkY","name":"Kletterkünstler","type":"talent","img":"icons/svg/